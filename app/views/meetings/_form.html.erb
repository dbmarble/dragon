<%= simple_form_for [@user, @meeting] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :logo_url %>
    <%= f.input :reason %>
    <%= f.input :location %>
    <%= f.input :date %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
